package tutorial15;
class Thing{
	public String name;
	public static String desc;
	
	public static void showInfo() {
		System.out.println("This is our information!");
	}
	
}

public class App {

	public static void main(String[] args) {
		Thing thing1=new Thing();
		thing1.name="something";
		Thing.desc="this is a thing";
		Thing.showInfo();

	}

}
