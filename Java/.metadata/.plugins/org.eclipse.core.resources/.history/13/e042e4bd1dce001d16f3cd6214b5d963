class Machine{
	public void start() {
		System.out.println("Machine started");
	}
}

class Camera extends Machine{

	@Override
	public void start() {
		System.out.println("Camera started");
	}
	
	public void snap() {
		System.out.println("Photo taken");
	}
	
}





public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
